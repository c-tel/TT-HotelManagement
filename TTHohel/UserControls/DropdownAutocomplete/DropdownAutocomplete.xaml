<UserControl x:Class="TTHohel.UserControls.DropdownAutocomplete.DropdownWithSearch"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TTHohel.UserControls.DropdownAutocomplete"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <ComboBox ItemsSource="{Binding FilteredItems, ElementName=DropdownWithSearch}" IsTextSearchEnabled="False" IsEditable="True"
              Text="{Binding SearchText, Mode= TwoWay}">
        
    </ComboBox>
</UserControl>
